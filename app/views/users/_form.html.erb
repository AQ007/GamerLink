
<%= form_with model: User.new, data: {remote: false} do |form| %>

<p>
  <%= form.label :username %><br>
  <%= form.text_field :username %>
</p>

<p>
  <%= form.label :email %><br>
  <%= form.text_field :email %>
</p>

<p>
  <%= form.label :password %><br>
  <%= form.password_field :password, class: 'form-control' %>
</p>

<p>
  <%= form.submit %>
</p>
<% end %>
