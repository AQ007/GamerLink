<div class="userpage">
  <div class="gamer">
  <h1>Here is a list of all of our players</h1>
  <li><% @users.each do |user| %></li>
  <li><%= link_to(user.username, user_path(user)) %></li>
  <% end %>
  <div class="card">
    <div class="container">
      <img src="https://unsplash.it/200" alt="Avatar" style="width:100%">
      <p>GamerLink</p>
    </div>
  </div>
</div>
